{
  "compilerOptions": {
    "outDir": "dist/server",
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "allowJs": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "outFile": undefined,
    "rootDir": "./server",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noEmit": false,
    "isolatedModules": false
  },
  "include": ["server"]
}
